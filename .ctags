--langdef=latex
--langmap=latex:.tex
--regex-latex=/\\label\{([^}]*)\}/\1/l,label/
--regex-latex=/\\section\{([^}]*)\}/\1/s,section/
--regex-latex=/\\subsection\{([^}]*)\}/. \1/s,section/
--regex-latex=/\\subsubsection\{([^}]*)\}/.   \1/s,section/
--regex-latex=/\\section\*\{([^}]*)\}/\1/s,section/
--regex-latex=/\\subsection\*\{([^}]*)\}/. \1/s,section/
--regex-latex=/\\subsubsection\*\{([^}]*)\}/.   \1/s,section/
